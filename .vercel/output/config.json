{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/categorias/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/productos/nuevo/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/productos/([^/]+?)/editar/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/productos/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/shop/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/shop/?$",
			"dest": "_render"
		}
	]
}